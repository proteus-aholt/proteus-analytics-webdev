<?xml version="1.0" encoding="UTF-8"?>
<miwt:container
        xmlns:miwt="http://cms.i2rd.com/xsd/miwt-template"
        xmlns="http://www.w3.org/1999/xhtml"
        xsi:schemaLocation="
    http://www.w3.org/1999/xhtml classpath:/com/i2rd/util/resources/xhtml1-i2rd.xsd
    http://cms.i2rd.com/xsd/miwt-template classpath:/net/proteusframework/ui/miwt/component/template/TemplateContainer.xsd"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        id="company-editor" element="div" class=""
        path="profile/company/CompanyValueEditor.xml">
    <div class="prop-column left">
        <div class="row">
            <miwt:component id="logos-property" element="div" class="" />
        </div>
    </div>

    <div class="prop-column right">
        <div class="row">
            <miwt:component id="name" element="div" class="" />
        </div>
        <div class="row">
            <miwt:component id="company-location" element="div" class="" />
        </div>
        <div class="row">
            <miwt:component id="website" element="div" class="" />
        </div>
        <div class="row">
            <miwt:component id="subdomain" element="div" class="" />
        </div>
        <div class="row">
            <miwt:component id="linkedin" element="div" class="" />
            <miwt:component id="twitter" element="div" class="" />
            <miwt:component id="facebook" element="div" class="" />
        </div>
    </div>
</miwt:container>